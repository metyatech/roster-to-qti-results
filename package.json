    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.8.1",
    "tsx": "^4.7.1",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.55.0"
